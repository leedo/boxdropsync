<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>org.lee.boxdropsync</string>
    <key>ProgramArguments</key>
    <array>
        <string>boxdropsync</string>
        <string>--local=~/Boxdrop/</string>
        <string>--remote=remote:~/boxdrop/</string>
        <string>--screenshots=remote:~/public_html/screenshots/</string>
        <string>--url=http://remote.com/~user/screenshots/</string>
    </array>
    <key>KeepAlive</key>
    <true/>
</dict>
</plist>
